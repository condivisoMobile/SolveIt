<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.levels.multi.MultiLevelFiveFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/counterIV"
            android:layout_width="100dp"
            android:layout_height="100dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.304"
            android:src="@drawable/counter" />

        <TextView
            android:id="@+id/counterTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/allerta_stencil"
            android:text="0"
            android:textSize="34sp"
            app:layout_constraintBottom_toBottomOf="@+id/counterIV"
            app:layout_constraintEnd_toEndOf="@+id/counterIV"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/counterIV"
            app:layout_constraintTop_toTopOf="@+id/counterIV"
            app:layout_constraintVertical_bias="0.497" />


        <ImageView
            android:id="@+id/buttonIV6"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@drawable/ic_red_button"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@+id/counterIV"
            app:layout_constraintStart_toStartOf="@+id/counterIV"
            app:layout_constraintTop_toBottomOf="@+id/counterIV" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>